[include]
  path = ~/themes.gitconfig
[user]
  email = valentin@nemirovsky.se
  name = Valentin Nemirovsky
[core]
  excludesfile = /Users/spacemonkey/.gitignore
  editor = nvim
[push]
  default = simple
[pull]
  rebase = true
[pager]
  diff = delta
  log = delta
  reflog = delta
  show = delta
[delta]
  features = calochortus-lyallii
[fetch]
	prune = true
[merge]
    tool = meld
[init]
    defaultBranch = master
